{
  "capabilities":
  [
    {
      "browserName": "operablink",
      "maxInstances": 5,
      "seleniumProtocol": "WebDriver",
      "webdriver.opera.driver": "C:/WebDriver/bin/operadriver.exe"
    },
    {
      "browserName": "firefox",
      "maxInstances": 5,
      "seleniumProtocol": "WebDriver",
      "webdriver.gecko.driver": "C:/WebDriver/bin/geckodriver.exe"
    },
    {
      "browserName": "chrome",
      "maxInstances": 5,
      "seleniumProtocol": "WebDriver",
      "webdriver.chrome.driver": "C:/WebDriver/bin/chromedriver.exe"
    },
    {
      "browserName": "internet explorer",
      "maxInstances": 5,
      "seleniumProtocol": "WebDriver",
      "webdriver.ie.driver": "C:/WebDriver/bin/IEDriverServer.exe"
    },
    {
      "browserName": "MicrosoftEdge",
	  "javascriptEnabled": true,
	  "acceptSslCerts": true,
      "maxInstances": 5,
      "seleniumProtocol": "WebDriver",
      "webdriver.edge.driver": 
	  "C:/WebDriver/bin/MicrosoftWebDriver.exe"
    }	
  ],
  "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
  "maxSession": 4,
  "port": 5556,
  "register": true,
  "registerCycle": 5000,
  "hub": "http://localhost:4444",
  "nodeStatusCheckTimeout": 2000,
  "nodePolling": 5000,
  "role": "node",
  "unregisterIfStillDownAfter": 60000,
  "downPollingLimit": 2,
  "debug": false,
  "servlets" : [],
  "withoutServlets": [],
  "custom": {}
}